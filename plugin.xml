<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Sample Category"
            id="IdealUnitSpecificationExperimental.commands.category">
      </category>
      <command
            name="Sample Command"
            categoryId="IdealUnitSpecificationExperimental.commands.category"
            id="IdealUnitSpecificationExperimental.commands.sampleCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="IdealUnitSpecificationExperimental.commands.sampleCommand"
            class="idealunitspecificationexperimental.sp.SPHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="IdealUnitSpecificationExperimental.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="IdealUnitSpecificationExperimental.toolbars.sampleToolbar">
            <command
                  commandId="IdealUnitSpecificationExperimental.commands.sampleCommand"
                  icon="icons/sample.gif"
                  tooltip="Say hello world"
                  id="IdealUnitSpecificationExperimental.toolbars.sampleCommand">
            </command>
         </toolbar>
      </menuContribution>
   </extension>

</plugin>
